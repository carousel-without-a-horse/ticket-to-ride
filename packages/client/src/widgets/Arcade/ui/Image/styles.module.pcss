.image {
  object-fit: contain;
}

.large {
  width: calc($arcadeSpace * 20px);
  height: calc($arcadeSpace * 20px);
}

.small {
  width: calc($arcadeSpace * 10px);
  height: calc($arcadeSpace * 10px);
}

.default {
  width: calc($arcadeGridSquareSize * 1px);
  height: calc($arcadeGridSquareSize * 1px);
}
