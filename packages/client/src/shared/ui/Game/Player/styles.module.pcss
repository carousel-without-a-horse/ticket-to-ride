.wrapper {
  height: 100%;
  width: 180px;
  margin-right: 10px;
  border-radius: 8px;
  padding: 10px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.icon {
  height: 60px;
  width: 60px;
  border-radius: 8px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.points {
  height: 60px;
  width: 40px;
  border-radius: 8px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
